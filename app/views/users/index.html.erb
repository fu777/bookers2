<%= render 'info', user: @user %>
<%= render 'books/form', book: @book %>

<div>
  <h2>Users</h2>
  <h4>image</h4>
  <h4>name</h4>
<% @users.each do |user| %>
  <p><%= image_tag user.get_profile_image(40, 40) %></p>
  <p><%= user.name %></p>
  <%= link_to 'show', user_path(current_user.id) %>
<% end %>
</div>
